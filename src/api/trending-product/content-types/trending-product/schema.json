{
  "kind": "collectionType",
  "collectionName": "trending_products",
  "info": {
    "singularName": "trending-product",
    "pluralName": "trending-products",
    "displayName": "TrendingProduct"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "brand": {
      "type": "string"
    },
    "productImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "position": {
      "type": "integer"
    },
    "linked_product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "trending_products"
    }
  }
}
