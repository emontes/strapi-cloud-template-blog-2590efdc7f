{
  "kind": "collectionType",
  "collectionName": "celebrities",
  "info": {
    "singularName": "celebrity",
    "pluralName": "celebrities",
    "displayName": "Celebrity"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "fullName"
    },
    "profession": {
      "type": "string",
      "required": true
    },
    "profileImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "country": {
      "type": "string"
    },
    "State": {
      "type": "string"
    },
    "City": {
      "type": "string"
    },
    "keyDetails": {
      "type": "component",
      "component": "celebrity.key-details",
      "repeatable": false,
      "required": false
    },
    "parings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::celebrity-product.celebrity-product",
      "mappedBy": "celebrity"
    }
  }
}
