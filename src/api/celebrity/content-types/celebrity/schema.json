{
  "kind": "collectionType",
  "collectionName": "celebrities",
  "info": {
    "singularName": "celebrity",
    "pluralName": "celebrities",
    "displayName": "Celebrity"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "fullName"
    },
    "profession": {
      "type": "string",
      "required": true
    },
    "profileImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "country": {
      "type": "string"
    },
    "State": {
      "type": "string"
    },
    "City": {
      "type": "string"
    },
    "keyDetails": {
      "type": "component",
      "component": "celebrity.key-details",
      "repeatable": false,
      "required": false
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "celebrities"
    },
    "sightings": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::celebrity-sighting.celebrity-sighting",
      "mappedBy": "celebrities"
    },
    "brandRelationships": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::celebrity-brand.celebrity-brand",
      "mappedBy": "celebrity"
    },
    "Seo": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    }
  }
}
